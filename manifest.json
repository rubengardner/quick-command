{
  "manifest_version": 3,
  "name": "Quick command",
  "version": "1.0",
  "description": "Access your favorite URLs quickly.",
  "action": {
    "default_popup": "popup.html"
  },
  "commands": {
    "open_popup": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y"
      },
      "description": "Open the Quick URL popup"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/content.js"]
    }
  ]
}
